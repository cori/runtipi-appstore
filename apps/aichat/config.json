{
  "$schema": "../schema.json",
  "name": "AI Chat",
  "available": true,
  "exposable": true,
  "port": 8765,
  "id": "aichat",
  "tipi_version": 1,
  "version": "v0.20.0",
  "categories": [
    "utilities",
    "ai"
  ],
  "description": "AI-powered chatbot application with a web interface",
  "short_desc": "AI-powered chatbot application",
  "author": "sigoden",
  "source": "https://github.com/sigoden/aichat",
  "form_fields": [
    {
      "name": "OPENAI_API_KEY",
