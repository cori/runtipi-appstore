      "required": false,
      "placeholder": "sk-proj-iamgroot",
      "min": 5,
      "max": 250
    },
    {
      "name": "OPENROUTER_API_KEY",
      "label": "OpenRouter API Key",
      "type": "password",
      "env_variable": "OPENROUTER_API_KEY",
      "required": false,
      "placeholder": "sk-or-iamgroot",
      "min": 5,
      "max": 250
    },
    {
      "name": "CLAUDE_API_KEY",
      "label": "Claude API Key",
      "type": "password",
      "env_variable": "CLAUDE_API_KEY",
      "required": false,
      "placeholder": "sk-ant-iamgroot",
      "min": 5,
      "max": 250
    }
  ],
  "supported_architectures": ["amd64", "arm64"]
}
